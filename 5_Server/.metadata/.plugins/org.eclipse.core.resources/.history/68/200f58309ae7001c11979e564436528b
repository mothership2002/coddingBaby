<entry key="mainBoardSelect">
SELECT BOARD_NO,BOARD_TITLE,TO_CHAR(머시기머시기),(댓글 카운트),(좋아요 카운트)
FROM BOARD
JOIN LIKE USING(BOARD_NO)
JOIN REPLY USING(BOARD_NO)
WHERE BOARD_TYPE = 생각을 좀 더 해봐야겠따
</entry>

